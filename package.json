{
  "name": "prodotype",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "author": "",
  "license": "ISC",
  "scripts": {
    "build": "npm run build:editor && npm run build:templates",
    "watch": "nodemon --watch 'src' --exec 'npm run build'",
    "reload": "reload -v -b -d pub/ -p 8080",
    "build:editor": "browserify src/js/Prodotype.js -o pub/prodotype.js -t [ babelify ] && cat node_modules/ejs/ejs.min.js >> pub/prodotype.js",
    "build:templates": "node src/js/BuildTemplates.js",
    "start": "http-server ."
  },
  "dependencies": {
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "ejs": "^2.5.1",
    "js-yaml": "^3.6.1",
    "material-ui": "^0.15.2",
    "react": "^15.2.0",
    "react-ace": "^3.5.0",
    "react-dom": "^15.2.0",
    "http-server": "^0.9.0",
    "react-tap-event-plugin": "^1.0.0"
  },
  "devDependencies": {
    "nodemon": "^1.10.0",
    "reload": "^1.0.1"
  }
}
